backend:
cors:
  allow-origin:
  allow-credentials:
  allow-methods:
  allow-headers:
proxy:
  port: 80
  api-prefix: "/"
grpc:
  max-call-recv-msg-size: 0
  max-call-send-msg-size: 0
gateway:
  use-jsonpb-v2-marshaler: false
  use-json-names: false
  emit-unpopulated: false
  keep-unknown: false

swagger:
  file: "{{.Swagger}}"

